{
  "name": "lemonsqueezy.ts",
  "version": "0.1.1",
  "description": "JavaScript / TypeScript SDK for the Lemon Squeezy API",
  "repository": {
    "type": "git",
    "url": "https://github.com/nurodev/lemonsqueezy.ts.git"
  },
  "homepage": "https://github.com/nurodev/lemonsqueezy.ts",
  "bugs": "https://github.com/nurodev/lemonsqueezy.ts/issues",
  "author": {
    "name": "nurodev",
    "email": "me@nuro.dev",
    "url": "https://nuro.dev"
  },
  "keywords": [
    "api",
    "lemonsqueezy",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./modules": {
      "import": "./dist/modules.mjs",
      "require": "./dist/modules.js"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "typesVersions": {
    "*": {
      "modules": [
        "./dist/modules.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "yarn build --watch",
    "build": "rimraf dist && tsup",
    "build:prod": "cross-env NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest watch --ui"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@vitest/ui": "^0.12.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "minifaker": "^1.34.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vitest": "^0.12.9"
  }
}
